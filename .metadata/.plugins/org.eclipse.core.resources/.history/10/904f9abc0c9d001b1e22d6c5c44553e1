public class PrinterManager {
	private PrinterManager mgr = null;
	
	public PrinterManager()
	{
		
	}
	
	public synchronized static PrinterManager getPrinterManager() {
		// TODO Auto-generated method stub
		if(mgr==null)
		{
			mgr = new PrinterManager();
		}
		return mgr;
	}
}
