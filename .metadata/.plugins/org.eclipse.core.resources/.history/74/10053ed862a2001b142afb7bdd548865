package chapter08_커맨드패턴.ch08_07;

import chapter08_커맨드패턴.ch08_07.command.MuteCommand;
import chapter08_커맨드패턴.ch08_07.command.PowerCommand;
import chapter08_커맨드패턴.ch08_07.command.TV;

public class Client {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		TV tv = new TV();
		PowerCommand pc = new PowerCommand(tv);
		MuteCommand mc = new MuteCommand(tv);
		TwoButtonController rc = new TwoButtonController();
		
		rc.setCommand(pc, mc);
		rc.button1Pressed();
		rc.button2Preesed();
		rc.button1Pressed();
		rc.button1Pressed();
		rc.button2Preesed();
		rc.button1Pressed();
		
	}

}
