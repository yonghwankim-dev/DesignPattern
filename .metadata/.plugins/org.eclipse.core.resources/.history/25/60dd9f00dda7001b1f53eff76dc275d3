package chapter10_데커레이터패턴.ch10_05;

public class Client {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		EMailContent simple = new BasicEmailContent("hello");	// 기본 메일
		
		EMailContent external = new ExternalEmailContent(simple);	// 외부메일
		
		EMailContent secure = new SecureEmailContent(simple);	// 암호 메일
		
		// 외부 메일 후에 암호 메일
		EMailContent secureAfterExternal;
	}

}
