package chapter13_추상팩토리패턴.ch13_02;

import chapter13_추상팩토리패턴.ch13_02.Door.Door;
import chapter13_추상팩토리패턴.ch13_02.Motor.HyundaiMotor;
import chapter13_추상팩토리패턴.ch13_02.Motor.LGMotor;
import chapter13_추상팩토리패턴.ch13_02.Motor.Motor;

public class DoorFactory {
	// vendorID에 따라 LGMotor 또는 HyundaiMotor 객체를 생성함
	public static Door createMotor(VendorID vendorID)
	{
		Door door = null;
		switch(vendorID)
		{
		case LG:
			door = new LGMotor();
			break;
		case HYUNDAI:
			door = new HyundaiMotor();
			break;
		}
		return door;
		
	}
}
