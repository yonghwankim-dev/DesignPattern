package chapter10_데커레이터패턴.ch10_06;

public class AirBagDecorator extends CarOptionDecorator{

	public AirBagDecorator(CarComponent decoratedCar) {
		super(decoratedCar);
	}

	@Override
	public int getPrice() {
		return super.getPrice() + getAirBagPrice();
	}

	@Override
	public String getCarinfo() {
		return super.getCarinfo();
	}

	private int getAirBagPrice()
	{
		return 1000;
	}
	
}
