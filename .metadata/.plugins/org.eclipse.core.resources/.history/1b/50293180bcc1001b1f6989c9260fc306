package chapter07_스테이트패턴.ch07_05;

public class Drinking {
	private int price;
	private int stock;
	
	public Drinking(int stock, int price) {
		// TODO Auto-generated constructor stub
		this.stock = stock;
		this.price = price;
	}
	
	public void dispense()
	{
		this.stock--;
	}

	public int getPrice() {
		return price;
	}

	public int getStock() {
		return stock;
	}

	
}
