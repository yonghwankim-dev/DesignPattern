package chapter06_싱글턴패턴.ch06_07;

import chapter06_싱글턴패턴.ch06_06.UserThead;

/**
 * 문제점 : 스레드를 사용할 경우 싱글턴 객체를 유지할 수 없음
 * 클래스 이름이 잘못됨 Singleton -> Printer
 * 
 * @author qkdlf
 *
 */
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		UserThead[] user = new UserThead[THREAD_NUM];
		for(int i=0;i<THREAD_NUM;i++)
		{
			user[i] = new UserThead((i+1)+"-thread");
			user[i].start();
		}
	}

}
