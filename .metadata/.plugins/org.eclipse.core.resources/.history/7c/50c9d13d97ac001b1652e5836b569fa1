package chapter14_컴퍼지트패턴.ch14_04;

public class File {
	private String name;
	private int size;
	private int depth = 0;
	
	public File(String name, int size) {
		this.name = name;
		this.size = size;
	}

	public String getName() {
		return name;
	}

	public int getSize() {
		return size;
	}

	public void setDepth(int depth) {
		this.depth = depth;
	}
	
	public void print(){
		for(int i=0;i<depth;i++)
		{
			System.out.print("\t");
		}
	}
	
	
}
