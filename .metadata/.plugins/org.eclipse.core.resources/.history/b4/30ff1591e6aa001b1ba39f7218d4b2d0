package chapter12_팩토리메서드패턴.ch12_07;

public abstract class CarState {
	protected Car car;
	protected CarState() {
		this.car = car;
	}
	
	public void speedDown(int targetSpeed)
	{
		System.out.print("Speed: " + car.getSpeed());
		if(targetSpeed < car.getSpeed())
		{
			car.setSpeed(targetSpeed);
		}
	}
}
