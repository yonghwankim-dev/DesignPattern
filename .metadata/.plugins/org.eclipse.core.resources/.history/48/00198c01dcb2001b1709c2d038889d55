package chapter03_SOLID¿øÄ¢.practice.p02_OCP_OpenClosedPrinciple.time.before;

public class TimeReminder {
	private MP3 m;
	public void setTimeProvider(TimeProvider tProv)
	{
		this.tProv = tProv;
	}
	
	public void reminder()
	{
		int hours = tProv.getTime();
		if(hours>=22)
		{
			m.playSong();
		}
	}
}
