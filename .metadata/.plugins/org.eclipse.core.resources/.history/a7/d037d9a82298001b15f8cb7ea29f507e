package chapter05_스트래티지패턴.ch05_07;

public class Item {
	private String name;
	private int price;
	
	
	
	public Item(String name, int price) {
		// TODO Auto-generated constructor stub
		this.name = name;
		this.price = price;
	}
	
	public Item()
	{
		
	}
	public String getName() {
		return name;
	}
	public int getPrice() {
		return price;
	}
	
	
}
